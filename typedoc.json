{
  "plugin": ["typedoc-plugin-markdown"],
  "entryPoints": [
    "./src/fivem/**/*.ts",
    "./src/redm/**/*.ts",
    "./src/server/**/*.ts"
  ],
  "readme": "none",
  "out": "docs",
  "enumMembersFormat": "table",
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "typeDeclarationFormat": "table",
  "indexFormat": "table",
  "useCodeBlocks": true,
  "expandObjects": true,
  "hidePageHeader": true
}
