#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules/lefthook/bin/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules/lefthook/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules/lefthook/bin/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules/lefthook/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/lefthook@1.11.3/node_modules:/home/runner/work/nativewrappers/nativewrappers/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lefthook/bin/index.js" "$@"
else
  exec node  "$basedir/../lefthook/bin/index.js" "$@"
fi
